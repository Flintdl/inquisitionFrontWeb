{
  "extends": ["next/core-web-vitals"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["next/babel"]
    }
  },
  "rules": {
    "import/first": "error",
    "import/no-duplicates": "error",
    "import/no-unresolved": "error",
    "import/no-unused-modules": "warn",
    "no-unused-vars": ["warn", { "varsIgnorePattern": "^_" }]
  }
}
